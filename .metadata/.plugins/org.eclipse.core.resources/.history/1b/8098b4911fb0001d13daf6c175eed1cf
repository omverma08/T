package com.basic;

public class Calculator {
	
	int num; //Decalre Global variable
	
	
	
	static int num1=10; ////Intialization
	
	
	int addition() {
		System.out.println(num1);
		return 0;
	}
	
	

	
	
	public static void main(String[] args) {
		num1=5; 	//Re-Assign
		System.out.println(num1);
	}
	
	

}
